<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.runemagic.gw2tools.gui.controller.CharactersController">
   <center>
      <TableView fx:id="tblCharacters" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="colCharName" prefWidth="131.0" text="Name" />
            <TableColumn fx:id="colCharProf" prefWidth="61.0" text="Profession" />
            <TableColumn fx:id="colCharLevel" prefWidth="35.0" text="Level" />
            <TableColumn fx:id="colCharRace" minWidth="0.0" prefWidth="73.0" text="Race" />
            <TableColumn fx:id="colCharGender" prefWidth="59.0" text="Gender" />
            <TableColumn fx:id="colCharGuild" prefWidth="126.0" text="Guild" />
            <TableColumn fx:id="colCharCreated" prefWidth="91.0" text="Created" />
            <TableColumn fx:id="colCharAge" minWidth="0.0" prefWidth="124.0" text="Age" />
            <TableColumn fx:id="colCharDeaths" prefWidth="57.0" text="Deaths" />
         </columns>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </TableView>
   </center>
</BorderPane>
